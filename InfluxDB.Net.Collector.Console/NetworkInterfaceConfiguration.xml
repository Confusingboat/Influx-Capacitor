<InfluxDB.Net.Collector>
  <CounterGroups>
    <CounterGroup Name="NetworkInterface" SecondsInterval="1">
      <Counter>
        <CategoryName>Network Interface</CategoryName>
        <CounterName>Bytes Sent/sec</CounterName>
        <InstanceName>*</InstanceName>
      </Counter>
    </CounterGroup>
  </CounterGroups>
</InfluxDB.Net.Collector>