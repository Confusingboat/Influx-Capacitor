<InfluxDB.Net.Collector>
  <CounterGroups>
    <CounterGroup Name="SqlBatchRequests" SecondsInterval="10">
      <Counter Name="BatchRequestsPerSec">
        <CategoryName>SQLServer:SQL Statistics</CategoryName>
        <CounterName>Batch Requests/sec</CounterName>
        <InstanceName></InstanceName>
      </Counter>
    </CounterGroup>
	<CounterGroup Name="SqlCompilations" SecondsInterval="10">
      <Counter Name="BatchRequestsPerSec">
        <CategoryName>SQLServer:SQL Statistics</CategoryName>
        <CounterName>Compilations/sec</CounterName>
        <InstanceName></InstanceName>
      </Counter>
    </CounterGroup>
	<CounterGroup Name="SqlConnections" SecondsInterval="10">
      <Counter Name="BatchRequestsPerSec">
        <CategoryName>SQLServer:General Statistics</CategoryName>
        <CounterName>User Connections</CounterName>
        <InstanceName></InstanceName>
      </Counter>
    </CounterGroup>
	<CounterGroup Name="SqlConnectionMemory" SecondsInterval="10">
      <Counter Name="BatchRequestsPerSec">
        <CategoryName>SQLServer:Memory Manager</CategoryName>
        <CounterName>Connection Memory (KB)</CounterName>
        <InstanceName></InstanceName>
      </Counter>
    </CounterGroup>
	<CounterGroup Name="SqlMemoryGrantsPending" SecondsInterval="10">
      <Counter Name="BatchRequestsPerSec">
        <CategoryName>SQLServer:Memory Manager</CategoryName>
        <CounterName>Memory Grants Pending</CounterName>
        <InstanceName></InstanceName>
      </Counter>
    </CounterGroup>
  </CounterGroups>
</InfluxDB.Net.Collector>